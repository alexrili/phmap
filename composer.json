{
  "name": "alexrili/phmap",
  "type": "library",
  "description": "Payload maps and transform. From A structure to B structure",
  "keywords": [
    "A to B",
    "payloadmap",
    "map",
    "payload"
  ],
  "homepage": "https://github.com/alexrili/phmap",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Lima",
      "email": "alexrili@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "alexrili/vephar": "^2.0",
    "illuminate/support": ">=9"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9"
  },
  "autoload": {
    "psr-4": {
      "Phmap\\Phmap\\": "src"
    },
    "files": [
      "./src/helpers.php",
      "./src/consts.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Phmap\\Phmap\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
